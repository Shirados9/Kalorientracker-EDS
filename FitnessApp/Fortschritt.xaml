<UserControl x:Class="FitnessApp.Fortschritt"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:FitnessApp"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="1200">
    
    <StackPanel Margin="10 10 0 0">
        <TextBox x:Name="ProteinsZiel" PreviewTextInput="NumberValidationTextBox" Text="120"/>
        <TextBox x:Name="CarbsZiel" PreviewTextInput="NumberValidationTextBox" Text="200"/>
        <TextBox x:Name="FatsZiel" PreviewTextInput="NumberValidationTextBox" Text="80"/>
    </StackPanel>
</UserControl>
